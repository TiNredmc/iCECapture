# Clock generated via on board STM32 clone that ack as USB iCELink on iCESugar nano board.
# set_io CLK 	D1

# Input pins for capturing
set_io CAP[0] B4
set_io CAP[1] C6
set_io CAP[2] B5
set_io CAP[3] E5
set_io CAP[4] E1
set_io CAP[5] C2
set_io CAP[6] B1
set_io CAP[7] A1

# UART<->FPGA
set_io TX	A3
set_io RX	B3

# Kind of ... Interrupt for host, but there's LED attach to it too, so active high xD.
set_io INT0 B6

# Capture clock at 48MHz from oscillator
set_io CAP_CLK C5